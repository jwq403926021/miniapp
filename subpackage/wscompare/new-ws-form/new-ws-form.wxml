            <button type="primary" bindtap="submitSurveyImage">上传图片</button>
        </view>
        <block wx:if="{{(role == 8 && (status == 20 || status == 33))}}">
            <view class="button-sp-area two-btn-wrap"> <!-- 客服 -->
                <button type="primary" bindtap="serviceSubmit" data-type="1">提交</button>
                <button bindtap="serviceSubmit" data-type="0">暂存</button>
            </view>
            <view class="button-sp-area"> <!-- 客服 -->
                <button type="primary" bindtap="serviceSubmit" data-type="2">驳回</button>
            </view>
        </block>

        <view wx:if="{{(role == 12 && (status == 39))}}" class="button-sp-area"> <!-- 施工人员 比价 -->
            <button type="primary" bindtap="compareCommit">提交比价</button>
        </view>
        <view wx:if="{{(role == 12 && (status == 13 || status == 43))}}" class="button-sp-area two-btn-wrap"> <!-- 施工人员 提交 暂存 -->
            <button type="primary" bindtap="workHandleWS">提交</button>
            <button data-save="true" bindtap="workHandleWS">暂存</button>
        </view>
        <view wx:if="{{role == 12 && status != 39}}" class="button-sp-area two-btn-wrap">
            <button type="primary" bindtap="workerSubmitComment">施工提交留言</button>
            <button type="primary" bindtap="workerSubmitImage">施工提交图片</button>
        </view>
        <view wx:if="{{((role == 5 || role == 6 || role == 7 || role == 12) && (status == 22))}}" class="button-sp-area">
            <button type="primary" bindtap="assignToOther">转办</button>
        </view>
<!--        <view class="button-sp-area"> &lt;!&ndash; 查勘员 申请开票 &ndash;&gt;-->
<!--            <button type="primary" bindtap="receiptApplySubmit">申请开票</button>-->
<!--        </view>-->
<!--        <view class="button-sp-area"> &lt;!&ndash; 授权 &ndash;&gt;-->
<!--            <button type="primary" bindtap="signSubmit">授权</button>-->
<!--        </view>-->
        <view class="button-sp-area two-btn-wrap" wx:if="{{role == 22 && (status == 11 || status == 50)}}"> <!-- 财务人员 提交 暂存 -->
            <button type="primary" bindtap="financeSubmit">提交</button>
            <button data-save="true" bindtap="financeSubmit">暂存</button>
        </view>
        <view class="button-sp-area two-btn-wrap" wx:if="{{(role == 33 && (status == 51))}}"> <!-- 核损人 审核 -->
            <button data-type="2" type="primary" bindtap="confirmSubmit">通过</button>
            <button data-type="1" bindtap="confirmSubmit">驳回</button>
        </view>
<!--        <view class="button-sp-area two-btn-wrap"> &lt;!&ndash; 市负责人 审核发票 &ndash;&gt;-->
<!--            <button data-type="1" type="primary" bindtap="receiptApproveSubmit">驳回</button>-->
<!--            <button data-type="2" type="primary" bindtap="receiptApproveSubmit">通过</button>-->
<!--        </view>-->
<!--        <view class="button-sp-area"> &lt;!&ndash; 财务人员 提交 发票 &ndash;&gt;-->
<!--            <button type="primary" bindtap="receiptImageSubmit">提交财务图片</button>-->
<!--        </view>-->
        <view class="button-sp-area" wx:if="{{role == 13 && status == 41}}">
            <button type="primary" bindtap="offerSubmitImage">报价提交图片</button>
        </view>
        <view class="button-sp-area" wx:if="{{role == 5 || role == 6 || role == 7}}">
            <button type="primary" bindtap="managerSubmitImage">负责人提交图片</button>
        </view>
        <view class="button-sp-area" wx:if="{{orderId != null}}">
            <button type="primary" bindtap="downloadImages">下载所有图片</button>
        </view>
        <view class="button-sp-area" wx:if="{{(role == 5 || role == 6 || role == 7) && status != null}}">
            <button type="primary" data-id="{{orderId}}" bindtap="openOperation">管理工单</button>
        </view>
    </view>
    <van-popup
            show="{{ show }}"
            position="bottom"
            overlay="{{ true }}"
    >
        <van-area area-list="{{ areaList }}" value="{{ region }}" bind:confirm="onConfirm" bind:cancel="onCancel"/>
    </van-popup>
    <van-popup
            show="{{ showreassign }}"
            position="bottom"
            overlay="{{ true }}"
    >
        <van-area columns-num="{{2}}" area-list="{{ areaList }}" value="{{ reassignRegion }}" bind:confirm="onConfirmReassign" bind:cancel="onCancel"/>
    </van-popup>
    <van-dialog
        message="请拨打出险方电话或者受损方电话。"
        title="提醒"
        show="{{ showWorkerHit }}"
    ></van-dialog>
    <plate-number-keyboard show="{{showKeyboard}}" bind:getResult="setNumber" bind:close="closePlateNum" initValue="{{plateNumber}}"></plate-number-keyboard>
    <van-action-sheet
            show="{{ showactionsheet }}"
            actions="{{ actions }}"
            bind:close="onactionsheetClose"
            bind:select="onactionsheetSelect"
    />
    <cover-view class="startYuyinImage" wx:if="{{recordState == true}}">
        <cover-view>开始语音</cover-view>
    </cover-view>
</view>

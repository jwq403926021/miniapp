                    </view>
                    <view class="weui-cell__bd readonly">
                        {{mail || '-'}}
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode" wx:if="{{weatherBill == '1'}}">
                    <view class="weui-cell__hd">

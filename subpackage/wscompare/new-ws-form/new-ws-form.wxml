<view class="page">
    <view class="page__bd">
        <view class="weui-cells__title">竞价{{insuranceType == '1' ? '物损工单' : '非车工单'}}</view>
        <view class="weui-cells weui-cells_after-title">
            <block wx:if="{{orderId}}">
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">工单号</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{orderId}}</text>
                    </view>
                    <view class="weui-cell__ft">
                        <view class="field-right-btn" data-content="{{orderId}}" bindtap="copy">
                            <van-icon name="column" />
                        </view>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">工单状态</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{statusMap[status] || '暂存'}}</text>
                    </view>
                </view>
            </block>

            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">区域</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <view class="picker weui-select weui-select_in-select-after" bindtap="openLocation">{{regionLabel ? regionLabel : '请填写地址'}}</view>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{regionLabel || '-'}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input weui-cell_vcode" wx:if="{{insuranceType == '1'}}">
                <view class="weui-cell__hd">
                    <view class="weui-label">车牌号</view>
                </view>
                <view class="weui-cell__bd" bind:tap="openPlatePicker" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <text class="weui-input readonly">{{plateNumber || '请输入车牌号'}}</text>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{plateNumber || '-'}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">报案号</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <input class="weui-input" bindinput="inputgetName" data-name='reportNumber'
                           value="{{reportNumber}}" placeholder="报案号"/>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{reportNumber || '-'}}</text>
                </view>
            </view>

            <block wx:if="{{!(role == 12 && status == 39 && budgetPreliminarySurvey > 10000)}}">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">出险方</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                        <input class="weui-input" bindinput="inputgetName" data-name='customerUser'
                               value="{{customerUser}}" placeholder="出险方姓名"/>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{customerUser || '-'}}</text>
                    </view>
                </view>

                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">沟通方式</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                        <input class="weui-input" bindinput="inputgetName" data-name='customerPhone'
                               value="{{customerPhone}}" placeholder="请输入电话号码"/>
                    </view>
                    <block wx:else>
                        <view class="weui-cell__bd">
                            <text class="weui-input readonly">{{customerPhone || '-'}}</text>
                        </view>
                        <view class="weui-cell__ft">
                            <view class="field-right-btn" data-content="{{customerPhone}}" bindtap="copy">
                                <van-icon name="column" />
                            </view>
                            <view class="field-right-btn" data-phone="{{customerPhone}}" data-worker="1" bindtap="dialPhone">
                                <van-icon name="phone"></van-icon>
                            </view>
                        </view>
                    </block>
                </view>
            </block>

            <block wx:if="{{!(role == 12 && status == 39 && budgetPreliminarySurvey > 10000)}}">
                <view class="weui-cell weui-cell_input" wx:if="{{insuranceType != '2'}}">
                    <view class="weui-cell__hd">
                        <view class="weui-label">受损方</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                        <input class="weui-input" bindinput="inputgetName" data-name='damagedUser'
                               value="{{damagedUser}}" placeholder="受损方姓名"/>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{damagedUser || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode" wx:if="{{insuranceType != '2'}}">
                    <view class="weui-cell__hd">
                        <view class="weui-label">沟通方式</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                        <input class="weui-input" bindinput="inputgetName" data-name='damagedPhone'
                               value="{{damagedPhone}}" placeholder="请输入电话号码"/>
                    </view>
                    <block wx:else>
                        <view class="weui-cell__bd">
                            <text class="weui-input readonly">{{damagedPhone || '-'}}</text>
                        </view>
                        <view class="weui-cell__ft">
                            <view class="field-right-btn" data-content="{{damagedPhone}}" bindtap="copy">
                                <van-icon name="column" />
                            </view>
                            <view class="field-right-btn" data-phone="{{damagedPhone}}" data-worker="1" bindtap="dialPhone">
                                <van-icon name="phone"></van-icon>
                            </view>
                        </view>
                    </block>
                </view>
            </block>

            <view class="weui-cell weui-cell_select">
                <view class="weui-cell__hd weui-cell__hd_in-select-after">
                    <view class="weui-label">承保方式</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <van-radio-group value="{{ acceptInsurance }}" custom-class="weui-input vertical" bind:change="onRadioChange" data-name="acceptInsurance">
                        <van-radio custom-class="fl mr5" name="1">异地</van-radio>
                        <van-radio custom-class="fl mr5" name="2">本地</van-radio>
                    </van-radio-group>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{acceptInsurance == 1 ? '异地' : '本地'}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label lh24">初步估损金额(查)</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <input class="weui-input" type="digit" bindinput="inputgetName" data-name='budgetPreliminarySurvey'
                           value="{{budgetPreliminarySurvey}}"  placeholder="初步估损金额(查)"/>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{(budgetPreliminarySurvey == null || budgetPreliminarySurvey == '') ? '-' : budgetPreliminarySurvey}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label lh24">受损方索赔金额(查)</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <input class="weui-input" type="digit" bindinput="inputgetName" data-name='damageMoneySurvey'
                           value="{{damageMoneySurvey}}"  placeholder="受损方索赔金额(查)"/>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{(damageMoneySurvey == null || damageMoneySurvey == '') ? '-' : damageMoneySurvey}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_select">
                <view class="weui-cell__hd weui-cell__hd_in-select-after">
                    <view class="weui-label">服务类型</view>
                </view>
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <picker bindchange="serviceTypeChange" value="{{serviceTypeValue}}"
                            range="{{serviceTypeList}}">
                        <view class="picker weui-select weui-select_in-select-after">
                            {{serviceTypeLabel || '请选择服务类型'}}
                        </view>
                    </picker>
                </view>
                <view class="weui-cell__bd" wx:else>
                    <text class="weui-input readonly">{{serviceTypeLabel}}</text>
                </view>
            </view>

            <view wx:if="{{role != 1 && role != 2 && role != 3 && role != 4}}" class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">保险公司</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input readonly">{{companyName || '-'}}</text>
                </view>
            </view>

            <view wx:if="{{role == 5 || role == 6 || role == 7 || role == 22}}" class="weui-cell weui-cell_select">
                <view class="weui-cell__hd weui-cell__hd_in-select-after">
                    <view class="weui-label">交强险</view>
                </view>
                <view class="weui-cell__bd">
                    <view data-index='{{index}}' data-id="{{orderId}}" data-type="0" data-value="{{isCompulsory}}" catchtap="setFinishCase" class="{{isCompulsory == 1 ? 'finished' : 'not-finish'}} fl"></view>
                </view>
            </view>
            <view wx:if="{{role == 5 || role == 6 || role == 7 || role == 22}}" class="weui-cell weui-cell_select">
                <view class="weui-cell__hd weui-cell__hd_in-select-after">
                    <view class="weui-label">商业险</view>
                </view>
                <view class="weui-cell__bd">
                    <view data-index='{{index}}' data-id="{{orderId}}" data-type="1" data-value="{{isBusiness}}" catchtap="setFinishCase" class="{{isBusiness == 1 ? 'finished' : 'not-finish'}} fl"></view>
                </view>
            </view>
        </view>

        <view class="weui-cells__title">案件信息</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                    <textarea maxlength="-1" wx:if="{{!show && !showKeyboard && !showreassign}}" class="weui-textarea" bindinput="inputgetName" data-name='information'
                              value="{{information}}" placeholder="请输入文本" style="height: 7em"/>
                </view>
                <view wx:else class="weui-cell__bd readonly" style="color:#808080; min-height: 7em; font-size:14px;">
                    {{information || '-'}}
                </view>
            </view>
            <view class="button-sp-area two-btn-wrap" wx:if="{{(role == 1 && (orderId == null || status == null || status == 21)) || (role == 8 && (status == 20 || status == 33))}}">
                <button class="yuyinBtn {{recordState == true ? 'yuyinBtnBg' : '' }}" data-name="information" bindtouchstart="recordStart" bindtouchend="recordEnd">
                    <text wx:if="{{!recordState}}">语音报案</text>
                    <text wx:else>松开结束</text>
                </button>
                <button class="yuyinBtn" bind:tap="digestRecord">
                    识别信息
                </button>
            </view>
        </view>

        <view class="weui-cells__title">案件图片</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">
                    <view class="weui-uploader">
                        <view class="weui-uploader__bd">
                            <view class="weui-uploader__files">
                                <view wx:if="{{liveImageFiles.length == 0 && orderId != null && status != null && !(role == 1 || role == 5 || role == 6 || role == 7 || role == 8)}}" class="no-image">
                                    无图片
                                </view>
                                <block wx:for="{{liveImageFiles}}" wx:key="index">
                                    <view class="weui-uploader__file image-preview-wrap" bindtap="previewImage" data-name="liveImageFiles" id="{{item.path}}">
                                        <image class="weui-uploader__img" src="{{item.path}}" mode="aspectFill"/>
                                        <van-icon wx:if="{{role == 1 || role == 5 || role == 6 || role == 7 || role == 8}}" catchtap="removeImage" data-name="liveImageFiles" data-id="{{item.id}}" data-index="{{index}}" name="clear" custom-class="delete-image-icon"></van-icon>
                                    </view>
                                </block>
                            </view>
                            <view class="weui-uploader__input-box" wx:if="{{role == 1 || role == 5 || role == 6 || role == 7 || role == 8}}">
                                <view class="weui-uploader__input" bindtap="chooseImage" data-name="liveImageFiles"></view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>

        <block wx:if="{{surveyUser && status != null && role != 10 && role != 11 && role != 12}}">
            <view class="weui-cells__title">查勘员信息</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">查勘员姓名</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{surveyUser || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">查勘员手机</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{surveyPhone || '-'}}</text>
                    </view>
                    <view class="weui-cell__ft">
                        <view class="field-right-btn" data-phone="{{surveyPhone}}" bindtap="dialPhone">
                            <van-icon name="phone"></van-icon>
                        </view>
                    </view>
                </view>
            </view>
        </block>
        <block wx:if="{{workerUser && role != 1 && role != 2 && role != 3 && role != 4 && role != 33 && status != 39}}">
            <view class="weui-cells__title">施工方信息</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">施工方姓名</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{workerUser || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">施工方手机</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{workerPhone || '-'}}</text>
                    </view>
                    <view class="weui-cell__ft">
                        <view class="field-right-btn" data-phone="{{workerPhone}}" bindtap="dialPhone">
                            <van-icon name="phone"></van-icon>
                        </view>
                    </view>
                </view>
            </view>
        </block>
        <block wx:if="{{serviceName}}">
            <view class="weui-cells__title">客服信息</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">客服姓名</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{serviceName || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">客服手机</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{servicePhone || '-'}}</text>
                    </view>
                    <view class="weui-cell__ft">
                        <view class="field-right-btn" data-phone="{{servicePhone}}" bindtap="dialPhone">
                            <van-icon name="phone"></van-icon>
                        </view>
                    </view>
                </view>
            </view>
        </block>

        <block wx:if="{{cityManagerName}}">
            <view class="weui-cells__title">平台负责人信息</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">平台负责人姓名</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{cityManagerName || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label">平台负责人手机</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{cityManagerMobile || '-'}}</text>
                    </view>
                    <view class="weui-cell__ft">
                        <view class="field-right-btn" data-phone="{{cityManagerMobile}}" bindtap="dialPhone">
                            <van-icon name="phone"></van-icon>
                        </view>
                    </view>
                </view>
            </view>
        </block>

        <block wx:if="{{surveyLossName}}">
            <view class="weui-cells__title">核损人信息</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">核损人姓名</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{surveyLossName || '-'}}</text>
                    </view>
                </view>
                <view class="weui-cell weui-cell_input">
                    <view class="weui-cell__hd">
                        <view class="weui-label">核损时间</view>
                    </view>
                    <view class="weui-cell__bd">
                        <text class="weui-input readonly">{{surveyLossTime || '-'}}</text>
                    </view>
                </view>
            </view>
        </block>

        <block wx:if="{{orderId != null && status != null}}">
            <view class="weui-cells">
                <view class="weui-cell weui-cell_select" wx:if="{{role != 1}}">
                    <view class="weui-cell__hd weui-cell__hd_in-select-after">
                        <view class="weui-label">处理方式</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 12 && (status == 13 || status == 43)) || (role == 22 && (status == 11 || status == 50))}}">
                        <van-radio-group value="{{ handlingType }}" custom-class="weui-input vertical" bind:change="onRadioChange" data-name="handlingType">
                            <van-radio custom-class="fl mr5" name="0">咨询</van-radio>
                            <van-radio custom-class="fl mr5" name="1">施工</van-radio>
                        </van-radio-group>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{handlingType ? ['咨询', '施工'][handlingType] : '-'}}</text>
                    </view>
                </view>

                <view class="weui-cell weui-cell_select" wx:if="{{handlingType == 0}}">
                    <view class="weui-cell__hd weui-cell__hd_in-select-after">
                        <view class="weui-label">受损方是否同意</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 12 && (status == 13 || status == 43))}}">
                        <van-radio-group value="{{ isAgree }}" custom-class="weui-input vertical" bind:change="onRadioChange" data-name="isAgree">
                            <van-radio custom-class="fl mr5" name="0">否</van-radio>
                            <van-radio custom-class="fl mr5" name="1">是</van-radio>
                        </van-radio-group>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{['否', '是'][isAgree] || '-'}}</text>
                    </view>
                </view>

                <view class="weui-cell weui-cell_select" wx:if="{{handlingType == 1 && role != 1}}">
                    <view class="weui-cell__hd weui-cell__hd_in-select-after">
                        <view class="weui-label">施工方式</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 12 && (status == 13 || status == 39 || status == 43))}}">
                        <van-radio-group value="{{ workType }}" custom-class="weui-input vertical" bind:change="onRadioChange" data-name="workType">
                            <van-radio custom-class="fl mr5" name="0">押金</van-radio>
                            <van-radio custom-class="fl mr5" name="1">授权</van-radio>
                            <van-radio custom-class="fl mr5" name="2">垫付</van-radio>
                        </van-radio-group>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{['押金', '授权', '垫付'][workType] || '-'}}</text>
                    </view>
                </view>

                <view class="weui-cell weui-cell_select" wx:if="{{handlingType != 0 && role != 1}}">
                    <view class="weui-cell__hd weui-cell__hd_in-select-after">
                        <view class="weui-label">验收</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 12 && (status == 13|| status == 43))}}">
                        <van-radio-group value="{{ isAcceptance }}" custom-class="weui-input vertical" bind:change="onRadioChange" data-name="isAcceptance">
                            <van-radio custom-class="fl mr5" name="0">否</van-radio>
                            <van-radio custom-class="fl mr5" name="1">是</van-radio>
                        </van-radio-group>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{['否', '是'][isAcceptance] || '-'}}</text>
                    </view>
                </view>

                <view class="weui-cell weui-cell_input weui-cell_vcode">
                    <view class="weui-cell__hd">
                        <view class="weui-label lh24">初步估损金额(施)</view>
                    </view>
                    <view class="weui-cell__bd" wx:if="{{(role == 12 && (status == 13 || status == 39 || status == 43))}}">
                        <input class="weui-input" type="digit" bindinput="inputgetName" data-name='budgetPreliminary'
                               value="{{budgetPreliminary}}"  placeholder="初步估损金额(施)"/>
                    </view>
                    <view class="weui-cell__bd" wx:else>
                        <text class="weui-input readonly">{{(budgetPreliminary == null || budgetPreliminary == '') ? '-' : budgetPreliminary}}</text>
                    </view>
                </view>


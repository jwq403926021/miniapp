    </van-popup>
    <van-popup
            show="{{ showreassign }}"
            position="bottom"
            overlay="{{ true }}"
    >
        <van-area columns-num="{{2}}" area-list="{{ areaList }}" value="{{ reassignRegion }}" bind:confirm="onConfirmReassign" bind:cancel="onCancel"/>
    </van-popup>
    <van-dialog
            message="请拨打出险方电话或者受损方电话。"

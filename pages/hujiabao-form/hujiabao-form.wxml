<wxs module="wxsUtil">
    function formatDateTime (str, isDate) {
      if (isDate) {
          return str.split('T')[0]
      } else {
          return str.replace('T', ' ').replace('Z', ' ')
      }
    }
    module.exports = {
        formatDateTime: formatDateTime
    }
</wxs>
